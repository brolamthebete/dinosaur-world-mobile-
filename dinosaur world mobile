loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

A1tab1:CreateButton("night version", function()
--[[ Subscribe to Zaptosis on YouTube for more awesome scripts!
 
